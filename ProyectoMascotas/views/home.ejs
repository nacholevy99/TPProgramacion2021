<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
	<link width="" rel="shortcut icon" href="/stylesheets/imagenes/logo-mercado-liebre.jpg" />
	<link rel="stylesheet" href="./stylesheets/bootstrap-grid.min.css">
	<title> Mercado Liebre Argentina</title>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
</head>
<body>
    <%- include('partials/header.ejs') %>

    	<!-- Banner -->
	<div class="banner">
		<img src="/stylesheets/imagenes/img-home-banner.jpg" alt="Banner de ofertas">
	</div>
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Novedades</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for (let index = 0; index < 4; index++) { %>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="localhost:3000/detalle">
						<figure class="product-box_image">
							<img src="/stylesheets/imagenes/products/default-image.png" alt="cafetera moulinex">
						</figure>
						<article class="product-box_data">
							<h2><%=  %></h2>
							<p><%= %></p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
			<% } %>
			
		</div>
	</div>
	<!-- /Novedades -->
	<!-- Más comentados -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Más comentados</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for (let index = 0; index < 4; index++) { %>
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="localhost:3000/detalle">
						<figure class="product-box_image">
							<img src="/stylesheets/imagenes/products/default-image.png" alt="cafetera moulinex">
						</figure>
						<article class="product-box_data">
							<h2>Nombre del producto</h2>
							<p>Lorem ipsum dolor sit amet consectetur, adipisicing elit. Eaque delectus ex</p>
							<p>Comentarios: 12</p>
						</article>
					</a>
				</section>
			</div>
			<% } %>
		</div>
	</div>
	<!-- /Más comentados -->


   

     <%- include('partials/footer.ejs') %>
</body>
</html>